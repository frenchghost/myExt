{
    "manifest_version": 2,
    "name": "Velocity Handler",
    "version": "1.0",
    "permissions": ["activeTab", "storage", "webNavigation", "webRequestBlocking", "http://*/*", "https://*/*", "storage", "tabs"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "all_frames": true,
        "js": ["main.js"]
      }
    ]
  }
  